generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Property{
    id        String  @id
  type      String
  bedrooms  Int
  price     Int
  tenure    String
  description String
  location  String
  picture   String
  AllPics   String[]
  FloorPlan String
  Postcode  String
  url       String
  added_month String
  added_day   Int
  added_year  Int
}
